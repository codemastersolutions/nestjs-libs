{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@nestjs-libs/better-auth": "<rootDir>/../libs/better-auth/src",
    "@nestjs-libs/better-auth/(.*)": "<rootDir>/../libs/better-auth/src/$1"
  }
}